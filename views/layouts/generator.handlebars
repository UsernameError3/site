<!doctype html>
<html>
<head>
    <meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" /> <!-- mobile viewport -->
    <title>HackSI :: {{title}}</title>
    <link rel="stylesheet" href="https://yui-s.yahooapis.com/pure/0.3.0/pure-min.css">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@hacksi">
    <meta name="twitter:image:src" content="https://hacksi.org/static/img/twitter-logo.jpg">
    <meta name="twitter:image" content="https://hacksi.org/static/img/twitter-logo.jpg">
    <meta property="og:title" content="Southern Illinois Hackathon"/>
    <meta property="og:url" content="https://hacksi.org/"/>
    <meta property="og:site_name" content="HackSI"/>
    <meta property="og:image" content="https://hacksi.org/static/img/logo.jpg"/>
    <meta property="og:type" content="blog"/>
    <meta property="og:description" content="The first ever Southern Illinois Hackathon. What can you build in 24 hours?"/>
    {{addCSS "/css/responsive-nav.css"}}
    {{addCSS "/css/social-icons.css"}}
    {{addCSS "/css/generator.css"}}
    {{{writeGeneratorCSS}}}
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/geopattern/1.2.3/js/geopattern.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.3/jquery.min.js"></script>
</head>
<body>
    <!-- {{{content}}} -->
<div class="container-row">
    <div class="box box-1">
        <div class="nav-collapse">
            {{> nav }}
        </div>
    </div>

    <div class="box box-2">
        <div class="container-column">
            <div class="left">
                <p>Seed:</p>
                <input type="text" id="string" value="start typing..." autocomplete="off" />

                <P>File:</P>
                <a id="save" href="#">Save Pattern...</a>
            </div>
            <div class="right">
                <div id="bg-0" class="js-geopattern"></div>
                <div id="bg-1" class="js-geopattern"></div>
            </div>
        </div>
    </div>

    <div class="box box-3">
        <div class="footer">
            <div class="pure-g">
                <div class="pure-u-1-2">&copy; 2013 - {{currentYear}} - HackSI - All rights reserved. - <a href="/code">Code of Conduct</a></div>
                <div class="pure-u-1-2 social">
                    Follow us on: 
                    <a href="/twitter" class="social-icon-twitter" title="Twitter"><span>Twitter</span></a>
                    <a href="/instagram" class="social-icon-instagram" title="Instagram"><span>Instagram</span></a>
                    <a href="/github" class="social-icon-github" title="Github"><span>Github</span></a>
                    <a href="/facebook" class="social-icon-facebook" title="Facebook"><span>Facebook</span></a>
                    <a href="/flickr" class="social-icon-flickr" title="Flickr"><span>Flickr</span></a>
                    <a href="https://www.ssllabs.com/ssltest/analyze.html?d=hacksi.org"><img src="https://sslbadge.org/?domain=hacksi.org" style="vertical-align: middle"></a>
                </div>
            </div>
            <div class="footer-nav">
            {{> nav }}
            </div>
        </div>
    </div>
</div>

<script src="/static/js/responsive-nav.js"></script>
<script src="/static/js/background-generator.js"></script>

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-39639122-2', 'hacksi.me');
  ga('send', 'pageview');
 
 var nav = responsiveNav(".nav-collapse");

</script>

</body>
</html>
